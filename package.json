    "react": "16.0.0-alpha.12",
    "react-native": "0.46.3",
    "react-native-button": "^2.0.0",
    "react-native-config": "^0.6.0",
    "react-native-maps": "^0.15.3",
    "react-native-navigation": "^1.1.143",
    "react-native-vector-icons": "^4.2.0",
    "react-redux": "^5.0.5",
